{
    "manifest_version": 3,
    "name": "Color Wheel",
    "version": "0.1",
    "action": {
        "default_popup": "interface.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/scripts/eyedropper.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}
  